{
  "name": "react-modal-portal",
  "version": "0.0.1",
  "description": "Customizable, optionally styled modal dialog component",
  "scripts": {
    "prepack": "rm -rf ./dist/ && parcel build --no-source-maps",
    "serve": "parcel serve examples/index.html --dist-dir .bin --open",
    "lint": "eslint . --fix --cache"
  },
  "pre-commit": "lint",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "targets": {
    "main": {
      "optimize": true
    },
    "module": {
      "optimize": true
    }
  },
  "keywords": [
    "react",
    "modal",
    "popup",
    "components",
    "dialog",
    "messagebox",
    "alert"
  ],
  "author": "Dominic Birch",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-mdx": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.25.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-config-xo-typescript": "^0.53.0",
    "eslint-plugin-react": "^7.31.10",
    "parcel": "latest",
    "pre-commit": "^1.2.2",
    "process": "^0.11.10",
    "typescript": "~4.7"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "styled-components": "^5.3.6"
  }
}
